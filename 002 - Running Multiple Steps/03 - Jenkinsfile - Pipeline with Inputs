pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
    stages {
        stage('build') {
            steps {
	    	 input {
                	message "Name?"
			parameters {
                    		string(name: 'NAME')
                	}
		}
		input{
                	message "Age?"
			parameters {
                    		string(name: 'AGE')
                	}
		}
                echo "$NAME"
                echo "$AGE"
            }
        }
    }
}
